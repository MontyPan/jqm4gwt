                <jqm:check><jqm:form.elements.JQMCheckbox name="cb11" text="Check2"/></jqm:check>
            </jqm:form.elements.JQMCheckset>
            <jqm:form.elements.JQMCheckset text="Vertical Checkset"> <!-- iconPos="RIGHT" -->
