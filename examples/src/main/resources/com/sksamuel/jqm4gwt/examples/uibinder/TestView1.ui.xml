            <jqm:form.elements.JQMCheckbox ui:field="cb3" name="cb3" text="Themed Checkbox" theme="b"/>
            <jqm:form.elements.JQMCheckbox ui:field="cb4" name="cb4" text="Right Icon and No corners" 
