              <jqm:cell>
                <jqm:button.JQMButton ui:field="setFlipValBtn" text="Set Value to 'Flip2'" mini="true" />
              </jqm:cell>
              <jqm:cell>
                <jqm:button.JQMButton ui:field="setFlipNullBtn" text="Set Value to 'null'" mini="true" />
              </jqm:cell>
            </jqm:layout.JQMTable>                                       
                                       
